pipeline {
    agent any    
    stages {
        stage('git clone') {
            steps {
              git branch: 'master', url: "https://github.com/ramesh-DevOps/Random_String.git"  
              sh "ls -ll"
            }
        }
        
        stage('Generate Password') {
            steps {
              sh "python3 random.py" 
            }
        }
    }
}
